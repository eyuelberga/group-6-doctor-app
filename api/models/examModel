const mongoose = require('mongoose');

const examSchema = new mongoose.Schema({
    examID:{
        type:String,
        required: true,
        unique:true
    },
    patientID:{
        type:String,
        required: true,
        unique:true

    },
    date:{
        type:Date,
        required:true,
        unique:false
    },
    keyFindings:{
        type:String,
        required:true,
        unique:false
    },
    brixiaScore:{
        type: Array,
        required:true,
        unique:false
    },
    imageURL:{
        type:String,
        required:true,
        unique:true,
    }
   


});
module.exports=mongoose.model('Exam',examSchema);
